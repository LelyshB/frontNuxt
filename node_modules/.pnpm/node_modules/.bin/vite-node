#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/home/sergey2/AstroAi_full/frontNuxt/node_modules/.pnpm/vite-node@3.2.4_jiti@2.6.0_lightningcss@1.30.1_terser@5.44.0_yaml@2.8.1/node_modules/vite-node/node_modules:/home/sergey2/AstroAi_full/frontNuxt/node_modules/.pnpm/vite-node@3.2.4_jiti@2.6.0_lightningcss@1.30.1_terser@5.44.0_yaml@2.8.1/node_modules:/home/sergey2/AstroAi_full/frontNuxt/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/home/sergey2/AstroAi_full/frontNuxt/node_modules/.pnpm/vite-node@3.2.4_jiti@2.6.0_lightningcss@1.30.1_terser@5.44.0_yaml@2.8.1/node_modules/vite-node/node_modules:/home/sergey2/AstroAi_full/frontNuxt/node_modules/.pnpm/vite-node@3.2.4_jiti@2.6.0_lightningcss@1.30.1_terser@5.44.0_yaml@2.8.1/node_modules:/home/sergey2/AstroAi_full/frontNuxt/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../vite-node/vite-node.mjs" "$@"
else
  exec node  "$basedir/../vite-node/vite-node.mjs" "$@"
fi
